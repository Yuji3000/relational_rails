<%= form_with url: "/stores/#{@store.id}", method: :patch, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name, :value => @store.name %>
  <%= form.label :open %>
  <%= form.text_field :open, :value => @store.open %>
  <%= form.label :sold_per_month %>
  <%= form.text_field :sold_per_month, :value => @store.sold_per_month %>
  <%= form.submit 'Edit Store' %>
<% end %>
