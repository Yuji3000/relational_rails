<%= form_with url: "/factories/#{@factory.id}/stores/new", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :open %>
  <%= form.text_field :open %>
  <%= form.label :sold_per_month %>
  <%= form.text_field :sold_per_month %>
  <%= form.submit 'Add Store' %>
<% end %>