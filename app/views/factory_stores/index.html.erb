<h1>Stores</h1>

  <h3><% @stores.each do |store| %></h3>
    <p>Name: <%= store.name %></p>
    <p>Open: <%= store.open %></p>
    <p>Sold per month: <%= store.sold_per_month %></p>
    <p>Created at: <%= store.created_at %></p>
    <p>Updated at: <%= store.updated_at %></p>
    <p>Sold per month: <%= store.sold_per_month %></p>
    <p>ID: <%= store.id %></p>
  <% end %>
<%= link_to "Create Store", "/factories/#{@factory.id}/stores/new" %>